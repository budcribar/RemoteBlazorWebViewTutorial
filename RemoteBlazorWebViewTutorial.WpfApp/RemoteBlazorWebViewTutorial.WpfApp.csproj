<Project Sdk="Microsoft.NET.Sdk.Razor">
  <Import Condition="Exists('$(MSBuildThisFileDirectory)/../../RemoteBlazorWebView/')" Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)/../../RemoteBlazorWebView/'))" />
  <PropertyGroup>
    <TargetFramework>net6-windows10.0.19041</TargetFramework>
    <TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
    <OutputType>WinExe</OutputType>
    <UseWPF>true</UseWPF>
    <Nullable>enable</Nullable>
    <IsShippingPackage>false</IsShippingPackage>
    <RazorLangVersion>3.0</RazorLangVersion>
    <Configurations>Debug;Release;Remote</Configurations>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="bin\Debug\net6-windows\wwwroot\_content\**" />
    <Compile Remove="publish\**" />
    <Content Remove="bin\Debug\net6-windows\wwwroot\_content\**" />
    <Content Remove="publish\**" />
    <EmbeddedResource Remove="bin\Debug\net6-windows\wwwroot\_content\**" />
    <EmbeddedResource Remove="publish\**" />
    <None Remove="bin\Debug\net6-windows\wwwroot\_content\**" />
    <None Remove="publish\**" />
    <Page Remove="bin\Debug\net6-windows\wwwroot\_content\**" />
    <Page Remove="publish\**" />
  </ItemGroup>
 
  <ItemGroup>
    <PackageReference Include="PeakSWC.RemoteableWebView" Version="6.0.8-preview5" />
    <PackageReference Include="PeakSWC.RemoteBlazorWebView.Wpf" Version="6.0.8-preview5" />
   
    <PackageReference Include="RemoteBlazorWebViewTutorial.Shared" Version="1.0.0" />
    <!--
   
  
   -->
  
  </ItemGroup>
 
  <ItemGroup>
    
   
  <!--
    <ProjectReference Include="..\..\RemoteBlazorWebView\src\RemoteableWebWindow\RemoteableWebView.csproj" />
    <ProjectReference Include="..\..\RemoteBlazorWebView\src\RemoteBlazorWebView.Wpf\RemoteBlazorWebView.Wpf.csproj" />
     <ProjectReference Include="..\RemoteBlazorWebViewTutorial.Shared\RemoteBlazorWebViewTutorial.Shared.csproj" />
  -->
   
  </ItemGroup>

  <ItemGroup>
    <Content Update="wwwroot\**">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  
</Project>